{
  "name": "sysya-wiki",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_CDN_URL": "https://media.sysya.com.au"
  },
  "regions": ["syd1"],
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}
